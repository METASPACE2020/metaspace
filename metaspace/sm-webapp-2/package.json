{
  "name": "sm-webapp-2",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "test:e2e": "start-server-and-test preview http://localhost:8082 'cypress run --e2e'",
    "test:e2e:dev": "start-server-and-test 'vite dev --port 4173' http://localhost:4173 'cypress open --e2e'",
    "test:cypress:open": "cross-env CYPRESS_RUN=true cypress open",
    "test:cypress": "cross-env CYPRESS_RUN=true cypress run",
    "test:unit": "vitest",
    "test:unit:dev": "cypress open --component",
    "type-check": "vue-tsc --noEmit -p tsconfig.cypress-ct.json --composite false",
    "format": "prettier --write src/",
    "dev": "cross-env NODE_ENV=development vite",
    "test": "yarn run generate-local-graphql-schema && cross-env NODE_ENV=test vitest",
    "test-ci": "cross-env NODE_ENV=test vitest --ci",
    "coverage": "cross-env NODE_ENV=test vitest --coverage",
    "deref-schema": "node ./deref_schema.js src/lib/metadataSchemas",
    "generate-local-graphql-schema": "cd ../graphql && yarn run gen-graphql-schema",
    "fetch-prod-graphql-schema": "apollo schema:download tests/utils/graphql-schema.json --endpoint https://metaspace2020.eu/graphql",
    "build": "yarn run deref-schema && cross-env NODE_ENV=production vite build",
    "build-ci": "cross-env NODE_ENV=production vite build",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "analyze-size": "rm -rf dist ; NODE_ENV=production WEBPACK_STATS=true vite build ; npx vite-bundle-visualizer dist/stats.json dist/ -p 9900 -O"
  },
  "dependencies": {
    "@apollo/client": "^3.8.6",
    "@sentry/vue": "^7.76.0",
    "@vue/apollo-composable": "^4.0.0-beta.11",
    "element-plus": "^2.4.1",
    "focus-visible": "^5.2.0",
    "graphql": "^16.8.1",
    "graphql-tag": "^2.12.6",
    "lodash-es": "^4.17.21",
    "subscriptions-transport-ws": "^0.11.0",
    "vite-svg-loader": "^4.0.0",
    "vue": "^3.3.4",
    "vue-gtag": "^2.0.1",
    "vue-router": "^4.2.5",
    "vue3-cookies": "^1.0.6",
    "vuex": "^4.0.2",
    "vuex-router-sync": "6.0.0-rc.1"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.23.2",
    "@fullhuman/postcss-purgecss": "^5.0.0",
    "@graphql-tools/mock": "^9.0.0",
    "@graphql-tools/schema": "^10.0.0",
    "@rushstack/eslint-patch": "^1.3.3",
    "@testing-library/vue": "^8.0.0",
    "@tsconfig/node18": "^18.2.2",
    "@types/node": "^18.18.5",
    "@vitejs/plugin-vue": "^4.4.0",
    "@vitejs/plugin-vue-jsx": "^3.0.2",
    "@vue/eslint-config-prettier": "^8.0.0",
    "@vue/eslint-config-typescript": "^12.0.0",
    "@vue/test-utils": "^2.4.1",
    "@vue/tsconfig": "^0.4.0",
    "autoprefixer": "^10.4.16",
    "cross-env": "^7.0.3",
    "cypress": "^13.3.1",
    "eslint": "8.22.0",
    "eslint-plugin-cypress": "^2.15.1",
    "eslint-plugin-vue": "^9.17.0",
    "jsdom": "^22.1.0",
    "json-schema-deref-sync": "^0.14.0",
    "node-fetch": "^3.3.2",
    "npm-run-all2": "^6.1.1",
    "postcss": "^8.4.31",
    "prettier": "^3.0.3",
    "sass": "^1.69.5",
    "start-server-and-test": "^2.0.1",
    "tailwindcss": "^3.3.5",
    "typescript": "~5.2.0",
    "unplugin-auto-import": "^0.16.7",
    "unplugin-vue-components": "^0.25.2",
    "vite": "^4.4.11",
    "vite-bundle-visualizer": "^0.10.0",
    "vite-plugin-virtual": "^0.2.0",
    "vitest": "^0.34.6",
    "vue-tsc": "^1.8.19"
  }
}
